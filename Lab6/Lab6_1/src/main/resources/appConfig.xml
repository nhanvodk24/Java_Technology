<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean id="product" class="vn.edu.tdtu.javatech.Lab6_1.Product" />
    <bean id="product1" class="vn.edu.tdtu.javatech.Lab6_1.Product" scope="prototype">
        <property name="name" value="Iphone" />
    </bean>
    <bean id="product2" class="vn.edu.tdtu.javatech.Lab6_1.Product" scope="prototype">
        <constructor-arg ref="product1" />
    </bean>

    <bean id="product3" class="vn.edu.tdtu.javatech.Lab6_1.Product" scope="singleton">
        <property name="name" value="Xbox" />
    </bean>
</beans>